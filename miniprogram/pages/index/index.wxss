
.swiperContaner{
  width: 100%;
  height: 400rpx;
  background-color: red;
}
.swiperContaner image{
  width: 100%;
  height: 100%;
}

.header{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}


.musicContaner{
  width: 100%;
  height: 150rpx;
  display: flex;
  flex-direction: row;
}

.left{
  width: 20%;
  height: 100%;
}
.left image{
  width: 90%;
  height: 90%;
  margin: 5%;
  border-radius: 10%;
}
.center{
  width: 60%;
}
.songName{
  color: black;
  font-size: 30rpx;
  padding-left: 24rpx;
  margin-top: 20rpx;
}
.singerName{
  color: black;
  font-size: 25rpx;
  padding-left: 24rpx;
  margin-top: 25rpx;
}
.right{
  width: 20%;
  height: 100%;
}
.right image{
  width: 40%;
  height: 40%;
  margin: 30%;
}
.v1{
  background-color: #c02c38;
}
.v2{
  background-color: #22a2c3;
}
.v3{
  background-color: #584717;
}



